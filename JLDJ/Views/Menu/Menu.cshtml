@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />

    <title>国网辽宁计量中心党建信息系统</title>
    <link rel="stylesheet" type="text/css" href="~/JS/jquery-easyui-1.7.0/themes/default/easyui.css">
    <link rel="stylesheet" type="text/css" href="~/JS/jquery-easyui-1.7.0/themes/icon.css">
    <link rel="stylesheet" type="text/css" href="~/JS/jquery-easyui-1.7.0/demo/demo.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Common.css">
    <script type="text/javascript" src="~/JS/jquery-easyui-1.7.0/jquery.min.js"></script>
    <script type="text/javascript" src="~/JS/jquery-easyui-1.7.0/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="~/JS/jquery-easyui-1.7.0/locale/easyui-lang-zh_CN.js"></script>
    <script type="text/javascript" src="~/JS/Common.js"></script>

</head>
<body>
    <div style="margin:00px 0;"></div>
    <div class="easyui-layout" style="width:auto;height:570px;">
        <div data-options="region:'north'" style="height:80px;background-color: #006569;" ><h1 style="color:#fff;text-align:center">国网辽宁计量中心党建信息系统</h1></div>
        <div data-options="region:'south',split:true" style="height:60px;background-color: #006569;color:#fff;"></br><center>技术支持：38193</center></div>

        <div data-options="region:'west',split:true" title="收起导航" style="width:100px;height:auto;">
            <div class="easyui-accordion" data-options="fit:true,border:false">
                <div title="系统管理" style="padding:10px;">
                    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="addtab(this,'../UserManage/UserManage')">用户管理</a>
                </div>
                <div title="信息管理" style="padding:10px;">
                    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="addtab(this,'../PersonManage/PersonManage')">人员管理</a>
                </div>
                <div title="业务管理" data-options="selected:true" style="padding:10px">
                    <a id="db" href="javascript:void(0)" class="easyui-linkbutton" onclick="addtab(this,'../MyWork/MyWork')">我的待办</a>

                    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="addtab(this,'../FlowSelect/FlowSelect')">流程查询</a>

                    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="addtab(this,'../FlowStart/FlowStart')">流程发起</a>

                </div>
            </div>
        </div>
        <div data-options="region:'center'" style="height:100%;" >
            <div class="easyui-tabs" id="tt" data-options="fit:true,border:false,plain:true" style="height:100%;">

            </div>
        </div>
        <script type="text/javascript">
            $(function () {
                $("#db").click();
            });
            function addtab(obj, url) {
                var addtitle = $(obj).linkbutton("options").text;
                var exi = $('#tt').tabs('exists', addtitle);
                var content = '<iframe scrolling="no" frameborder="0"  marginwidth="0" marginheight="0" allowtransparency="yes" src="' + url + '" style="width:100%;height:100%;"></iframe>';
                if (!exi)
                    $('#tt').tabs('add', {
                        title: addtitle,
                        content: content,
                        closable: true
                    });
                else $('#tt').tabs('select', addtitle);
            }
                    //function removePanel() {
                    //    var tab = $('#tt').tabs('getSelected');
                    //    if (tab) {
                    //        var index = $('#tt').tabs('getTabIndex', tab);
                    //        $('#tt').tabs('close', index);
                    //    }
                    //}
        </script>
</body>
</html>